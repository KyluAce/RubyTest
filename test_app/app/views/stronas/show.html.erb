<div class = "hero_unit">
<h1>Show Page</h1>

<table class = "table table-striped">


	<tr>
	<th>Category:</th>
	<td> <%= @strona.category.name%>  </td>
	</tr>

	<tr>
	<th>Name:</th>
	<td> <%= @strona.name%>  </td>
	</tr>

	<tr>

	<th>Position:</th>
	<td> <%= @strona.position%>  </td>

	</tr>
	<tr>

	<th>Visibility:</th>
	<td> <%= @strona.visibility ? "Yes" : "No"%>  </td>

	</tr>	

	<tr>

	<th>Created at:</th>
	<td> <%= @strona.created_at%>  </td>

	</tr>

	<tr>

	<th>Updated at:</th>
	<td> <%= @strona.updated_at%>  </td>

	</tr>


</table> 
<div style="text-align: center">
<%= link_to("Back", {:action => 'index'}, :class => 'btn btn-primary')%>
</div>
</div>