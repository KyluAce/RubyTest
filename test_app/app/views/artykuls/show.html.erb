<div class = "hero_unit">
<h1>Show Article</h1>

<table class = "table table-striped">


	<tr>
	<th>Article:</th>
	<td> <%= @artykuls.strona.name%>  </td>
	</tr>

	<tr>
	<th>Name:</th>
	<td> <%= @artykuls.name%>  </td>
	</tr>

	<tr>
	<th>Position:</th>
	<td> <%= @artykuls.position%>  </td>
	</tr>

	<tr>
	<th>Visibility:</th>
	<td> <%= @artykuls.visibility ? "Yes" : "No"%>  </td>
	</tr>	

	<tr>
	<th>Image:</th>
	<td> <%= image_tag(@artykuls.images)%>  </td>
	</tr>

	<tr>
	<th>Content:</th>
	<td> <%= @artykuls.content%>  </td>
	</tr>

	<tr>
	<th>Created at:</th>
	<td> <%= @artykuls.created_at%>  </td>
	</tr>

	<tr>
	<th>Updated at:</th>
	<td> <%= @artykuls.updated_at%>  </td>
	</tr>


</table> 
<div style="text-align: center">
<%= link_to("Back", {:action => 'index'}, :class => 'btn btn-primary')%>
</div>
</div> 