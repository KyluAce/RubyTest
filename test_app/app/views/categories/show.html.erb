<div class = "hero_unit">
<h1>Show Category</h1>

<table class = "table table-striped">
<tr>

	<th>Name:</th>
	<td> <%= @category.name%>  </td>

	</tr>
	<tr>

	<th>Position:</th>
	<td> <%= @category.position%>  </td>

	</tr>
	<tr>

	<th>Visibility:</th>
	<td> <%= @category.visibility ? "Yes" : "No"%>  </td>

	</tr>	

	<tr>

	<th>Created at:</th>
	<td> <%= @category.created_at%>  </td>

	</tr>

	<tr>

	<th>Updated at:</th>
	<td> <%= @category.updated_at%>  </td>

	</tr>


</table> 
<div style="text-align: center">
<%= link_to("Back", {:action => 'index'}, :class => 'btn btn-primary')%>
</div>
</div>